@{
    ViewData["Title"] = "Home Page";
}
<style>
    .hidden {
        display: none;
    }

    .success {
        color: #00B050;
    }

    .warning {
        color: #FFC000;
    }

    .danger {
        color: #FF0000
    }
</style>
<div class="app-main__inner">
    <div class="app-page-title">
        <div class="page-title-wrapper">
            <div class="page-title-heading">
                <div class="page-title-icon">
                    <i class="pe-7s-paperclip icon-gradient bg-mean-fruit">
                    </i>
                </div>
                <div>
                    Dashboard - Transaction In-Progress
                    <div class="page-title-subheading">
                        Dashboard for all current transactions.
                    </div>
                </div>
            </div>
            <div class="page-title-actions">
                
                <div class="d-inline-block dropdown">
                    <button onclick="javascript: printPage();" type="button" class="btn-shadow btn btn-info">
                        <span class="btn-icon-wrapper pr-2 opacity-7">
                            <i class="fa fa-print fa-w-20"></i>
                        </span>
                        Print Payment
                    </button>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="mb-3 progress hidden" id="processor">
                <div class="progress-bar progress-bar-animated bg-danger progress-bar-striped" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%;"></div>
            </div>
            <div class="main-card mb-3 card">
                <div class="card-header">
                    Active Transactions
                    
                </div>
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                        <thead>
                            <tr>
                                <th class="text-center">#</th>
                                <th class="text-center">TRANSACTION NUMBER</th>
                                <th class="text-center">DESCRIPTION</th>
                                <th class="text-center">TRANSACTION INITIATION DATE</th>
                                <th class="text-center">BUYER PAYMENT DATE</th>
                                <th class="text-center">SELLER BANK</th>
                                <th class="text-center">BUYER BANK</th>
                                <th class="text-center">TEMPORARY STATUS</th>
                                <th class="text-center">FINAL STATUS</th>
                                @*<th class="text-center">AMOUNT</th>*@
                                <th class="text-center">PAY TO</th>
                                <th class="text-center">BRIGHO PAYMENT DATE</th>
                                <th class="text-center">PROCESSED BY</th>
                                <th class="text-center">ACTION</th>
                            </tr>
                        </thead>
                        <tbody id="tbody">
                        </tbody>
                    </table>
                </div>
                <div class="d-block text-center card-footer">

                </div>
            </div>
        </div>
    </div>

</div>
<div id="dvContents" class="hidden">
    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
        <thead>
            <tr>
                <th class="text-center">#</th>
                <th class="text-center">TRANSACTION NUMBER</th>
                <th class="text-center">DESCRIPTION</th>
                <th class="text-center">SELLER PHONE</th>
                <th class="text-center">SELLER BANK</th>
                <th class="text-center">BUYER PHONE</th>
                <th class="text-center">BUYER BANK</th>
                <th class="text-center">STATUS</th>
                <th class="text-center">AMOUNT</th>
                <th class="text-center">PAY TO</th>
            </tr>
        </thead>
        <tbody id="tbodyPrint">
        </tbody>
    </table>
</div>
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p class="mb-0">Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled.</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="inprogress" />
<script type="text/javascript">
    document.getElementById("li-inprog").className = 'mm-active';
</script>
