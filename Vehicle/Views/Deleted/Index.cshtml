
@{
    ViewData["Title"] = "Index";
}

<style>
    .hidden {
        display: none;
    }

    .success {
        color: #00B050;
    }

    .warning {
        color: #FFC000;
    }

    .danger {
        color: #FF0000
    }
</style>
<div class="app-main__inner">
    <div class="app-page-title">
        <div class="page-title-wrapper">
            <div class="page-title-heading">
                <div class="page-title-icon">
                    <i class="pe-7s-trash icon-gradient bg-mean-fruit">
                    </i>
                </div>
                <div>
                    Dashboard - Deleted Transactions
                    <div class="page-title-subheading">
                        Dashboard for all Deleted Transactions.
                    </div>
                </div>
            </div>

        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="mb-3 progress hidden" id="processor">
                <div class="progress-bar progress-bar-animated bg-danger progress-bar-striped" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%;"></div>
            </div>
            <div class="main-card mb-3 card">
                <div class="card-header">
                    Deleted Transactions

                </div>
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                        <thead>
                            <tr>
                                <th class="text-center">#</th>
                                <th class="text-center">TRANSACTION NUMBER</th>
                                <th class="text-center">DESCRIPTION</th>
                                <th class="text-center">TRANSACTION INITIATION DATE</th>
                                <th class="text-center">SELLER PHONE</th>
                                <th class="text-center">SELLER BANK</th>
                                <th class="text-center">BUYER PHONE</th>
                                <th class="text-center">BUYER BANK</th>
                                <th class="text-center">TEMPORARY STATUS</th>
                                <th class="text-center">FINAL STATUS</th>
                                <th class="text-center">PROCESSED BY</th>
                            </tr>
                        </thead>
                        <tbody id="tcanbody">
                        </tbody>
                    </table>
                </div>
                <div class="d-block text-center card-footer">

                </div>
            </div>
        </div>
    </div>

</div>

<input type="hidden" id="deleted" />

<script type="text/javascript">
    document.getElementById("li-deleted").className = 'mm-active';
</script>



